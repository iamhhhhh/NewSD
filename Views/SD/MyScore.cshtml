@model IEnumerable<NewSD.Models.Score>
<div class="col-lg-6 col-md-6 col-sm-12">
    <div style="overflow:auto;height:640px">
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>No.</th>
                        <th style="text-align:center">วันที่</th>
                        <th>เวลาเริ่มต้น</th>
                        <th>เวลาสิ้นสุด</th>
                        <th>เวลารวม(นาที)</th>
                        <th>สถานที่</th>
                        <th>Work Status</th>
                    </tr>
                </thead>
                <tbody>
                    @{int rowNo = 0;}
                    @if (Model != null)
                    {
                        foreach (var score in Model)
                        {
                            <tr>
                                <td>@{rowNo++;} @rowNo</td>
                                <td style="width:100px">@score.SD_FDateScore.ToString("dd/MM/yyyy")</td>
                                <td>@score.SD_TimeStart.ToString("HH:MM")</td>
                                <td>@score.SD_TimeEnd.ToString("HH:MM")</td>
                                <td>@score.SD_TotalTime</td>
                                <td>@score.SD_Location</td>
                                <td>@score.SD_WorkHour</td>
                            </tr>
                                        }
                                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
